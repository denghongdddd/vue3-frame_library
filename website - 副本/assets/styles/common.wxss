ore{transform:skewX(45deg) translateX(200%)}.demo-block .vague .btn:before,.demo-block .vague .btn:after{width:30px;height:10px;background:#ff1f71;border-radius:10px;transition:0.5s;transition-delay:0s;content:" ";position:absolute;left:50%;transform:translateX(-50%);box-shadow:0 0 5px #ff1f71, 0 0 15px #ff1f71, 0 0 30px #ff1f71, 0 0 60px #ff1f71}.demo-block .vague .btn:before{bottom:-5px}.demo-block .vague .btn:after{top:-5px}.demo-block .vague .btn a{position:absolute;top:0;left:0;width:100%;height:100%;display:flex;justify-content:center;align-items:center;background:rgba(255,255,255,0.05);box-shadow:0 15px 35px rgba(0,0,0,0.2);border-top:1px solid rgba(255,255,255,0.1);border-bottom:1px solid rgba(255,255,255,0.1);border-radius:30px;color:#fff;z-index:1;font-weight:400;letter-spacing:1px;text-decoration:none;overflow:hidden;transition:0.5s;backdrop-filter:blur(5px) contrast(80%) brightness(90%)}.demo-block .vague .btn a:before{content:" ";position:absolute;top:0;left:0;width:50%;height:100%;background:linear-gradient(to left, rgba(255,255,255,0.15), transparent);transform:skewX(45deg) translateX(0);transition:0.5s}
